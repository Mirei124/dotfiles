def_key "j"
  scroll_down

def_key "k"
  scroll_up

def_key "f"
  page_down

def_key "b"
  page_up

def_key ","
  seek_backward

def_key "."
  seek_forward

def_key "ctrl-g"
  jump_to_position_in_song

def_key "g"
  move_home

def_key "G"
  move_end

def_key "O"
  jump_to_browser

def_key "O"
  jump_to_playlist_editor

def_key "]"
  next

def_key "["
  previous

def_key "n"
  next_found_item

def_key "N"
  previous_found_item

def_key "enter"
  enter_directory

def_key "enter"
  toggle_output

def_key "enter"
  run_action

def_key "enter"
  play_item

def_key "space"
  require_screen "playlist"
  select_item

def_key "space"
  add_item_to_playlist

def_key "space"
  toggle_lyrics_update_on_song_change

def_key "space"
  toggle_visualization_type

def_key "l"
  require_screen "playlist"
  jump_to_playing_song
  scroll_down
  move_selected_items_to
  remove_selection

def_key "m"
  move_sort_order_up

def_key "m"
  move_selected_items_up

def_key "M"
  move_sort_order_down

def_key "M"
  move_selected_items_down

def_key "ctrl-m"
  move_selected_items_to
